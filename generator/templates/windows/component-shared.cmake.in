# Import target "@COMPONENT@" for configuration "@BUILD_TYPE@"
set_property(TARGET @COMPONENT@ APPEND PROPERTY IMPORTED_CONFIGURATIONS @BUILD_TYPE_UPPER@)
set_target_properties(@COMPONENT@ PROPERTIES
    IMPORTED_IMPLIB_@BUILD_TYPE_UPPER@ "${_IMPORT_PREFIX}/lib/@IMPNAME@"
    IMPORTED_LOCATION_@BUILD_TYPE_UPPER@ "${_IMPORT_PREFIX}/bin/@FILENAME@"
)

list(APPEND _cmake_import_check_targets @COMPONENT@)
list(APPEND _cmake_import_check_files_for_@COMPONENT@ "${_IMPORT_PREFIX}/lib/@FILENAME@")